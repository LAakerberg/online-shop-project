    "react-icons": "^4.11.0",
    "react-router-dom": "^6.16.0",
    "styled-components": "^5.3.11",
    "yup": "^1.3.2"
  },
  "devDependencies": {
    "@types/react": "^18.2.27",
    "@types/react-dom": "^18.2.12",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.51.0",
    "eslint-plugin-react": "^7.33.2",
    "husky": "^8.0.3",
    "lint-staged": "^13.3.0",
    "postcss": "^8.4.31",
    "postcss-nesting": "^12.0.1",
    "prettier": "^2.8.8",
    "tailwindcss": "^3.3.3",
    "vite": "^4.4.11"
